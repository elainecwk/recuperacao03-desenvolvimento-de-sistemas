generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Biblioteca {
  id               String   @id
  isbn             String
  title            String
  authors          String[]
  publication_year String
  page_count       String
  created_at       DateTime
  updated_at       DateTime
  is_active        Boolean // SE FOR APENAS DESATIVAR

  @@map("Biblioteca")
}
